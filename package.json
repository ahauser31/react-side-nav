{
  "name": "react-side-nav",
  "version": "0.0.4",
  "description": "A side bar react component that supports custom styles, icons and works with react-router",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahauser31/react-side-nav.git"
  },
  "scripts": {
    "test-simple": "jest",
    "test-details": "jest --verbose",
    "test-watch": "jest --watch",
    "test": "jest --coverage",
    "build": "webpack --config webpack.dist.config.js",
    "start": "NODE_ENV=development webpack-serve webpack.config.js"
  },
  "keywords": [
    "react",
    "react-component",
    "nav bar",
    "side nav",
    "navigation",
    "side navigation",
    "menu"
  ],
  "author": "Andreas Hauser <andreas_hauser@gmx.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ahauser31/react-side-nav/issues"
  },
  "homepage": "https://github.com/ahauser31/react-side-nav",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "stage": 0
      },
      "autoprefixer": {},
      "cssnano": {}
    }
  },
  "serve": {
    "content": "./example/",
    "hot": true,
    "http2": false
  },
  "engines": {
    "node": "10.x.x"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1"
  },
  "peerDependencies": {
    "react": "^16.0",
    "react-dom": "^16.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browserslist": "^3.2.7",
    "codecov": "^3.0.2",
    "css-loader": "^0.28.11",
    "cssnano": "^4.0.0-rc.2",
    "enzyme": "^3.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.1",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "path": "^0.12.7",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-preset-env": "^5.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "regenerator-runtime": "^0.10.3",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3",
    "webpack-serve": "^0.3.2"
  }
}
